{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Automatic",
  "version": "0.1.0",
  "identifier": "com.velvet.automatic",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Automatic",
        "width": 1400,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/velvet-tiger/automatic/releases/latest/download/latest.json"
      ],
      "pubkey": "TODO: run `cargo tauri signer generate -w ~/.tauri/automatic.key` and paste the public key here"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
